{
    "env": {
        "browser": true,
        "es2021": true,
        "jquery": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["unused-imports"],
    "rules": {
        "no-unused-vars": "off",
        "unused-imports/no-unused-imports": "error",
        "unused-imports/no-unused-vars": [
            "warn",
            {
                "vars": "all",
                "varsIgnorePattern": "^_",
                "args": "after-used",
                "argsIgnorePattern": "^_"
            }
        ],
        "indent": [1, "tab"],
        "no-tabs": 0,
        "no-console": "off",
        "func-names": "off",
        "no-mixed-spaces-and-tabs": 0,
        "import/prefer-default-export": "off"
    },
    "ignorePatterns": ["../dist/", "../node_modules/", "webpack.config.js", "**/*.json", "../js/libs/", "**/*.min.js",  "../js/template-parts/blocks/via/"]
}